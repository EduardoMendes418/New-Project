                                    <p>{{item.descricao | limitTo:100}}{{ item.descricao.length >100 ? '' : ''}}</p>
                                   <div class="row">
                                        <div class=" col-4 col-sm-12">
                                           <!-- <h5 style="opacity: 0.5">R$ {{item.preco}} </h5> -->
                                            <h1 class="card-title pricing-card-title" style="font-size: 18px; font-weight: bold">
                                                    R$ {{item.preco}}
                                                <small class="text-muted">
                                                    <span  class="fa fa-plus-circle" aria-hidden="true" title="Adicionar ao carrinho"  ng-click="adicionarItens(item.codProduto,item.produto,item.preco)"></span>
                                                </small>
                                            </h1>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        <!--/*******************
