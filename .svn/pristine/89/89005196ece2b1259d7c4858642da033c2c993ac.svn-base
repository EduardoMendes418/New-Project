                      <img  src="images/lista-produtos/carrinho.png" alt="25px" height="25px">
                       <p> Seu Pedido </p>
                    </div>
                    <div class="panel-body">
                        <div ng-show="cartEmpty == 1" style="text-align: center;margin-bottom: 10%">
                            <span style="color:#c3c3c3">Seu carrinho está vazio</span>
                        </div>
                        <div class="row" ng-repeat="y in cart" class="{{'tr'+ y.cod}}" >
                            <div class="col-md-4" style="text-align: center;">
                                <span  class="fa fa-minus-circle" aria-hidden="true" title="Retirar" ng-click="minus(y.cod,y.qtd,y.preco)"></span>
                                <input class="inputValor1" type="text"  readonly ng-model="qtd" ng-value="{{y.qtd}}" placeholder="{{y.qtd}}" style="text-align:center; width:15px;border: none;position: relative;
                                bottom: 2px;"/>
                                <span class="fa fa-plus-circle" aria-hidden="true" ng-click="plus(y.cod,y.qtd)" title="Adicionar"></span>
                            </div>
                            <div class="col-md-4" >
                                <span class="" title="{{y.produto}}" style="font-size:95%" >{{y.produto | limitTo:14}}</span>
                            </div>
                            <div class="col-md-4" style="text-align: left">
                                <span>R$ {{y.subtotal}}</span>
                            </div>
                        </div> 

                        <div class="row lista" ng-show="divCart == 1">
                            <div>
                                <div class="col-md-8 textCart fontSize">Subtotal:</div>        
                                <div class="col-md-4 text-left" id="subtotal" ng-show="subTotal()" ><span style="font-size:100%">R$ {{subTotal()}}</span></div>
                        
                                <div class="col-md-8 textCart fontSize">Taxa de entrega:</div>                         
                                <div class="col-md-4 text-left"  ng-show="taxa"><span style="font-size:100%">R$ {{taxa}}</span></div>
                        
                                <div class="col-md-8 textCart font" >Total:</div>
                                <div class="col-md-4 text-left font" id="taxaEntrega"  ng-show="somaTotal">
                                    <span style="font-size:100%">R$ {{somaTotal}}</span>
                                </div>
                            </div>                        
                            <div  class="col-md-12" id="alterarEndereco" style="margin-bottom: 2%">
                                <input type="text" class="form-control" id="destino" name="destino" gm-places-autocomplete ng-model="destino"  
                                placeholder="Informe o endereço para entrega"   onfocusout="validarNumber(this.value)"  style="font-size: 12px !important" />
                            </div>
                            <!-- somente para restaurantes -->
                            <div ng-if="loja.categoria == 59" data-toggle="modal" data-target="#modalObs" id="obsPedido" style="margin-top: 5px;cursor: pointer" >
                                <i class="fa fa-pencil-square-o" aria-hidden="true"></i> Fazer observação para esse pedido ? 
                            </div>
                        </div> 
                    </div>
                    <div>
                      <!-- botão finalizar pedidos -->
                        <button class="btn btnFinalizar" ng-click="finalizarPedido()" disabled="disable" id="ttt">
                            Finalizar Compra
                        </button>
                        <input type="button" name="" id="btnfinalizarPedido" style="display: none" data-toggle="modal"  data-target="#exampleModalCenter">
                    </div>   

                <!--/*******************
                    * MODAL BTN LOGIN  *
                    ********************--> 
                    <div class="modal fade modal-login" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                            <div class="container">
                                    <div class="col-md-4 col-md-offset-4 col-sm-8 col-sm-offset-2" data-ng-show="!vm.signIn">
