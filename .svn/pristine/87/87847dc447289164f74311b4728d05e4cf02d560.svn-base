<script>
    /* funcao banner */
    function stepbanner(id){
        if(id == 1 ){

        $('.farmacia').hide(); 
        $('.delivery').hide();
        $('.buscaentrega').html();
        $('.buscaentrega').fadeIn();

        }else if(id == 2 ){

        $('.empresahome').hide(); 
        $('.farmacia').hide();
        $('.buscarestaurante').html();
        $('.buscarestaurante').fadeIn();
           
        }else if(id == 3){

        $('.delivery').hide(); 
        $('.empresahome').hide();
        $('.buscafarmacia').html( );
        $('.buscafarmacia').fadeIn();

        }else if (id == 4){

        $('.delivery').fadeIn(); 
        $('.empresahome').fadeIn();
        $('.farmacia').fadeIn();
        $('.buscafarmacia').hide();
        $('.buscarestaurante').hide();
        $('.buscaentrega').hide();

        }
    }

