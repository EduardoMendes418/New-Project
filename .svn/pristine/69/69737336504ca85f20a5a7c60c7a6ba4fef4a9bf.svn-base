<script>
  /* funcao banner desktop  abre e fecha  */
  function stepbanner(id){
    if(id == 1 ){

      $('.bike').hide(); 
      $('.moto').hide();
      $('.buscaentrega').html();
      $('.buscaentrega').fadeIn();

    }else if(id == 2 ){

      $('.bike').hide();
      $('.buscarestaurantee').html();
      $('.buscarestaurantee').fadeIn();

    }else if(id == 3){

      $('.moto').hide(); 
      $('.buscafarmaciaa').html( );
      $('.buscafarmaciaa').fadeIn();

    }else if (id == 5){

      $('.moto').fadeIn(); 
      $('.logo').fadeIn();
      $('.bike').fadeIn();
      $('.buscafarmaciaa').hide();
      $('.buscarestaurantee').hide();

    }

        //funcao  mobile quando clicar no banner  automaticamente  ele vai subir ao topo
        $(document).ready(function() {
          $('.price').click(function(event) {
            event.preventDefault();
            $('html, body').animate({scrolltop: 0},  0);
            event.stopPropagation();
          });
        });
      }

      function SomenteNumero(e) {
        var tecla = (window.event) ? event.keyCode : e.which;
        if ((tecla > 47 && tecla < 58)) return true;
        else {
          if (tecla == 8 || tecla == 0) return true;
          else return false;
        }
      }
      function SomenteLetra(e) {
        var tecla = (window.event) ? event.keyCode : e.which;
        if ((tecla > 47 && tecla < 58)) return false;
        else {
          if (tecla == 8 || tecla == 0) return true;
          else return true;
        }
      }

      $(function(){
        $("[data-toggle='bottom']").click(function(){
          $("html, body").animate({ scrollTop: $(document).height() }, );
        });
      });


    </script>

