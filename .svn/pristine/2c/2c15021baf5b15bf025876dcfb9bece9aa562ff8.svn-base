<!--Contato-->
<section >
    <div class="container-contato" id="contato">
            <div class="container">
                <div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto" >
                    <div class="row">
                        <div class="formulario">
                            <div class="col-sm-3"></div>
                            <div class="col-sm-4"></div>
                            <div class="col-sm-5">
                                <h1>PREEENCHA OS DADOS &
                                    RECEBA O CONTATO DA NO
                                    NOSSA EQUIPE
                                </h1>
                                <form name="form" class="form" novalidate>
                                    <div class="row">
                                        <div class="form-group col-md-12">
                                            <input class='form-control ' placeholder="Nome:" required ng-model="demail.nome" type='text' name="nome" onkeypress="return SomenteLetra(event);" 
                                            ng-class="{'is-invalid': form.nome.$invalid && form.nome.$touched,'is-valid': form.nome.$valid}" >
                                            <p ng-show="form.nome.$invalid && form.nome.$touched" style="color:red;font-size:10px !important;">Nome obrigatório</p>
                                        </div>
                                        <div class="form-group col-md-12">
                                            <input class='form-control ' placeholder="Email:" required ng-model="demail.email" type='email' name="email" ui-br-email-mask 
                                            onkeypress="return SomenteLetra(event);" ng-class="{'is-invalid': form.email.$invalid && form.email.$touched,'is-valid': form.email.$valid}">
                                            <p ng-show="form.email.$invalid && form.email.$touched" style="color:red;font-size:10px !important;">E-mail obrigatório</p>
                                        </div>
                                        <div class="form-group col-md-12">
                                            <input type="text" capitalize class="form-control" name="empresa" placeholder="Empresa"
                                                ng-model="demail.empresa"  ng-class="{'is-valid': form.empresa.$valid && form.empresa.$touched}" required >
                                            <p ng-show="form.empresa.$invalid && !form.empresa.$pristine" 
                                             class="help-block" style="color:red;font-size:10px !important;">Empresa é obrigatório.
                                            </p>
                                        </div>
                                        <div class="form-group col-md-12" ng-class="{ 'is-invalid' : form.cnpj.$invalid && !form.cnpj.$pristine }">
                                            <input type="text" capitalize class="form-control" placeholder="CNPJ" name="cnpj" id="cnpj" ng-model="demail.cnpj" ui-br-cnpj-mask ng-class="{'is-invalid': form.cnpj.$invalid && form.cnpj.$touched,'is-valid': form.cnpj.$valid}" required>
                                            <p ng-show="form.cnpj.$invalid && !form.cnpj.$pristine" 
                                            class="help-block" style="color:red;font-size:10px !important;">Campo obrigatório, digite um CNPJ válido.
                                            </p>
                                        </div>
                                        <div class="form-group col-md-9" ng-class="{ 'is-invalid' : form.telefone.$invalid && !form.telefone.$pristine }">
                                            <input type="text" capitalize class="form-control" name="telefone" id="telefone" placeholder="DDD + Telefone"
                                            ng-model="demail.telefone" ui-br-phone-number ng-class="{'is-invalid': form.telefone.$invalid && form.telefone.$touched,'is-valid': form.telefone.$valid}" required>
                                            <p ng-show="form.telefone.$invalid && !form.telefone.$pristine" 
                                            class="help-block" style="color:red;font-size:10px !important;">Telefone é obrigatório.
                                            </p>
                                        </div>
                                        <div class="col-md-3 col-md-offset-6 " ng-show="!cadastroOk">
                                            <button type="submit"  class="btn btn-primary" ng-click="enviarEmail()" ng-disabled="form.$invalid" ng-show="!cadTrue">Enviar</button>
                                        </div>
                                    </div>
                                </form>   
                            </div>
                        </div> 
                    </div>
                    <!--Telefone-->
                    <div class="telefone">
                        <div class="row">
                            <div class="col-12 col-sm-6 col-lg-3"></div>
                            <div class="col-12 col-sm-6 col-lg-4"></div>
                            <div class="col-12 col-sm-6 col-lg-5">
                                <span href="#">Telefone: (19) 98187-0077</span>
                            </div>    
                        </div>
                        <div class="row">
                            <div class="col-12 col-sm-6 col-lg-3"></div>
                            <div class="col-12 col-sm-6 col-lg-4"></div>
                            <div class="col-12 col-sm-6 col-lg-5">
                                <span href="#">Contato: 0800 941 3950</span>
                            </div>    
                        </div>
                    </div>    
                    <div class="row">
                        <div class="footer">
