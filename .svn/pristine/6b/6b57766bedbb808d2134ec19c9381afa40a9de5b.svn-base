                    <div class="col-sm-6 col-md-12">
                        <div class="media">
                            <div class="media-left logo-empresa">
                                <a href="#">
                                    <img src="images/lista-produtos/madero.png" >
                                    <!-- <img class="img-rounded"  ng-src="https://www.ddtotal.com.br/imgPublicidade/ec/{{loja.logo}}" style="max-width: 200px !important">-->
                                </a>
                            </div>
                            <div class="media-body" id="lojaInfo">
                                <h2 class="media-heading">{{loja.nomeFantasia}}</h2>                                
                                <p>{{loja.logradouro}}</p>
                                <p></p>
                            </div>
                        </div>
                    </div> 
                    <div class="col-sm-6 col-md-2"></div>
            <!--/*******************
                *  carrinho mobile * 
                ********************-->
                <div class="col-md-12">
                    <div class="col-md-4"></div>
                    <div class="col-md-4"  style="text-align: center">
                        <button type="button" class="btn btn-carrinho" data-toggle="modal" data-target="#exampleModal" data-whatever="@mdo">
                            <div class="panel-heading-mobile">
                            <img  src="images/lista-produtos/carrinho.png" alt="25px" height="25px">
                                <p> Seu Pedido </p>
                            </div>
                        </button>
                    </div>
                    <div class="col-md-4"></div>
                </div>
                <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">
                        <div class="modal-dialog" role="document">
                        <div class="modal-content">
                        <div class="panell panel-default" >
                                <div class="panel-heading">
                                    <img  src="images/lista-produtos/carrinho.png" alt="25px" height="25px">
                                    <p> Seu Pedido </p>
                                </div>

                                <div class="panel-body">
                                    <div class="row" ng-repeat="y in cart" class="{{'tr'+ y.cod}}" >

                                        <ul style="display: block">
                                            <li style="display: inline-block">
                                                <div class="col-md-4" style="text-align: center;">    
                                                    <span  class="fa fa-minus-circle" aria-hidden="true" title="Retirar" ng-click="minus(y.cod,y.qtd,y.preco)"></span>
                                                    <input class="inputValor1" type="text"  readonly ng-model="qtd" ng-value="{{y.qtd}}" placeholder="{{y.qtd}}" style="text-align:center;width: 15px;border: none;position: relative;
                                                    bottom: 2px;"/>
                                                    <span class="fa fa-plus-circle" aria-hidden="true" ng-click="plus(y.cod,y.qtd)" title="Adicionar"></span>
                                                </div>
                                            </li>
                                            <li style="display: inline-block">
                                                <div class="col-md-4" >
                                                    <span class="" title="{{y.produto}}" style="font-size:95%" >{{y.produto | limitTo:14}}</span>
                                                </div>
                                            </li>
                                            <li style="display: inline-block">
                                                <div class="col-md-4" style="text-align: left">
                                                    <span>R$ {{y.subtotal}}</span>
                                                </div>
                                            </li> 
                                        </ul>
                                    </div> 
            
                                    <div class="row lista" >
                                        <div >
                                            <ul style="display: block">
                                                <li style="display: inline-block">
                                                    <div class="col-sm-8 textCart fontSize">Subtotal:</div>   
                                                </li>
                                                <li style="display: inline-block">
                                                    <div class="col-sm-4 text-left" id="subtotal" ng-show="subTotal()" ><span style="font-size:100%">R$ {{subTotal()}}</span></div> 
                                                </li>
                                            </ul>
                                            <ul style="display: block">
                                                <li style="display: inline-block">
                                                    <div class="col-sm-8 textCart fontSize">Taxa de entrega:</div>  
                                                </li>
                                                <li style="display: inline-block">
                                                    <div class="col-sm-4 text-left"  ng-show="taxa"><span style="font-size:100%">R$ {{taxa}}</span></div>
                                                </li>
                                            </ul>
                                            <ul style="display: block">
                                                <li style="display: inline-block">
                                                    <div class="col-sm-8 textCart font" >Total:</div>
                                                </li>
                                                <li style="display: inline-block">
                                                    <div class="col-sm-4 text-left font" id="taxaEntrega"  ng-show="somaTotal"><span style="font-size:100%">R$ {{somaTotal}}</span></div>
                                                </li>
                                            </ul>
                                        </div>

                                        <div ng-if="loja.categoria == 59" data-toggle="modal" data-target="#modalObs" id="obsPedido" style="margin-top: 5px;cursor: pointer" >
                                            <i class="fa fa-pencil-square-o" aria-hidden="true"></i> Fazer observação para esse pedido ? 
                                        </div>
                                    </div> 
                                </div>
                                
                                <div class="" style="">
                                    <button class=" btn btnFinalizar" ng-click="finalizarPedido(2)" data-toggle="modal" id="finalizarPedido2">
                                        Finalizar Compra
                                    </button>
                                </div>    
                            </div>
                        </div>
                    </div>
                </div>    
                
            <!--/*******************
                *     Produtos     * 
                ********************--> 
                <div class="col-md-12">
                    <div class="col-md-12 pesquisa" id="detalhe">
                        <div class="row">       
                            <div class="col-md-12">
                                <div class="input-group">
                                    <input class="form-control " type="search" placeholder="Filtrar Produto..." name="search"  ng-model="cmpnome" >
                                    <div class="input-group-addon" style="cursor: pointer"><i class="fa fa-search"></i></div>
                                </div> 
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6" ng-repeat="item in grid | filter:{produto:cmpnome}">
                        <ul class="media-list">
                            <li class="media">
                                <div class="media-left lanche" >
                                    <a href="#">
                                        <img src="images/lista-produtos/lanche.png">
                                        <!--<img class="rounded" width="100" ng-src="https://www.ddtotal.com.br/imgPublicidade/ec/{{item.img}}">-->
                                    </a>
                                </div>
                                <div class="media-body">
                                    <h4 class="media-heading">{{item.produto}}</h4>
                                    <p>{{item.descricao | limitTo:100}}{{ item.descricao.length >100 ? '' : ''}}</p>
                                
                                   <div class="row">
                                        <div class=" col-4 col-sm-6">
                                            <h5 style="opacity: 0.5">R$ {{item.preco}} </h5>
                                        </div>
                                        <div class=" col-4"></div>
                                        <div class=" col-4 col-sm-6">
                                            <h4>
                                                <span  class="fa fa-plus-circle" aria-hidden="true" title="Adicionar ao carrinho"  ng-click="adicionarItens(item.codProduto,item.produto,item.preco)"></span>
                                            </h4>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--/*******************
                *      sidebar     * 
                ********************-->
            <div class="col-md-4 order-md-1  mb-4 blog-sidebar"  >
                <div class="panel panel-default" >
                    <div class="panel-heading">
                      <img  src="images/lista-produtos/carrinho.png" alt="25px" height="25px">
                       <p> Seu Pedido </p>
                    </div>
                    <div class="panel-body">
                        <div class="row" ng-repeat="y in cart" class="{{'tr'+ y.cod}}" >
                            <div class="col-md-4" style="text-align: center;">
                                <span  class="fa fa-minus-circle" aria-hidden="true" title="Retirar" ng-click="minus(y.cod,y.qtd,y.preco)"></span>
                                <input class="inputValor1" type="text"  readonly ng-model="qtd" ng-value="{{y.qtd}}" placeholder="{{y.qtd}}" style="text-align:center;width: 15px;border: none;position: relative;
                                bottom: 2px;"/>
                                <span class="fa fa-plus-circle" aria-hidden="true" ng-click="plus(y.cod,y.qtd)" title="Adicionar"></span>
                            </div>
                            <div class="col-md-4" >
                                <span class="" title="{{y.produto}}" style="font-size:95%" >{{y.produto | limitTo:14}}</span>
                            </div>
                            <div class="col-md-4" style="text-align: left">
                                <span>R$ {{y.subtotal}}</span>
                            </div>
                        </div> 

                        <div class="row lista" >
                            <div>
                                <div class="col-md-8 textCart fontSize">Subtotal:</div>        
                                <div class="col-md-4 text-left" id="subtotal" ng-show="subTotal()" ><span style="font-size:100%">R$ {{subTotal()}}</span></div>
                        
                                <div class="col-md-8 textCart fontSize">Taxa de entrega:</div>                         
                                <div class="col-md-4 text-left"  ng-show="taxa"><span style="font-size:100%">R$ {{taxa}}</span></div>
                        
                                <div class="col-md-8 textCart font" >Total:</div>
                                <div class="col-md-4 text-left font" id="taxaEntrega"  ng-show="somaTotal"><span style="font-size:100%">R$ {{somaTotal}}</span></div>
                            </div>
                            <!-- somente para restaurantes -->
                            <div ng-if="loja.categoria == 59" data-toggle="modal" data-target="#modalObs" id="obsPedido" style="margin-top: 5px;cursor: pointer" >
                                <i class="fa fa-pencil-square-o" aria-hidden="true"></i> Fazer observação para esse pedido ? 
                            </div>
                        </div> 
                    </div>
                    <div class="" style="">
                      <!-- botão finalizar pedidos -->
                      <button class=" btn btnFinalizar" ng-click="finalizarPedido(1)" data-toggle="modal" id="finalizarPedido1">
                            Finalizar Compra
                    </button>
                    </div>    
                </div>
            </div>   
        </div>
    </div>   
