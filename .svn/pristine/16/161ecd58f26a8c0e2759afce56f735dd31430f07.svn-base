<!-- ***********##################### FOMRULARIO   LOCACAO ##################**************** -->
<section>
  <div class="modal fade bd-example-modal-lg3" id="modal-locacao"  aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header" style="text-align: center !important;"  >  
          <img src="images/locacao/logo.png"  > 
          <a  class="close" data-dismiss="modal" aria-label="Close" ng-click="modalDefault">
            <span aria-hidden="true">×</span>
          </a>                            
        </div>
        <div class="modal-body" id="bloco1" >
          <div class="container-fluid">
            <div class="row ">
              <div class="col-md-12 formulario" >
                <form id="msform">
                 <div class="form-group col-sm-12 col-md-8">
                  <label>Nome Completo: *</label>
                  <input type="text" capitalize class="form-control " name="nome" id="nome" ng-model="proposta.nome" required  ng-click="clearInput()" autocomplete="off">
                </div>
                <div class="form-group col-sm-12 col-md-4">
                  <label>CPF: * </label>
                  <input type="text"  ng-model="proposta.cpf" name="cpf" id="cpf1" class="form-control" ui-br-cpf-mask  ng-click="clearInput()">                              
                </div>  
                <div class="form-group col-sm-12 col-md-6">
                  <label >Celular *</label>
                  <input type="text" capitalize class="form-control" name="celular" id="celular" ng-model="proposta.celular" ui-br-phone-number ng-minlength="15" ng-click="clearInput()"/>
                </div>
                <div class="form-group col-sm-12 col-md-6">
                  <label >E-mail *</label>
                  <input type="email"  name="email"  id="email"  ng-model="proposta.email" class="form-control" ng-click="clearInput()"/>
                </div>
                <div class="form-group col-sm-12 col-md-6">
                 <label >Veículo *</label>
                 <select class="form-control" name="tipoVeiculo" id="tipoVeiculo" ng-model="proposta.tipoVeiculo" ng-options="item1.tipo as item1.veiculo for item1 in tipoVeiculo" ng-click="clearInput()">                                         
                 </select>
               </div>
               <div class="form-group col-sm-12 col-md-6">
                <label >Modelo *</label>
                <select class="form-control" name="modeloVeiculo" id="modeloVeiculo" ng-model="proposta.modeloVeiculo" ng-options="item2.idModelo as item2.modelo for item2 in modeloVeiculo | filter:{ tipo : proposta.tipoVeiculo }" ng-click="clearInput()">
                  <option value="">-- SELECIONE --</option>                            
                </select>
              </div>
              <div ng-if="proposta.tipoVeiculo == 2" class="form-group col-sm-12 col-md-6" ng-init="proposta.acessorio = 0">
                <label >Acessório *</label>
                <select class="form-control" name="acessorio" id="acessorio" ng-model="proposta.acessorio" ng-click="clearInput()">                            
                  <option value="0"> Sem Acessório </option>
                  <option value="1"> Baú </option>
                 <!-- <option value="2">Capacete </option>
                  <option value="3">Baú,Capacete </option>-->
                </select>
              </div>
              <div ng-if="proposta.tipoVeiculo == 2" class="form-group col-sm-12 col-md-6" ng-init="proposta.tipoProtecao = 3">
                <label >Proteção *</label>
                <select class="form-control" name="tipoProtecao" id="tipoProtecao" ng-model="proposta.tipoProtecao" ng-options="modelo.idProtecao as modelo.protecao for modelo in tipoProtecao" ng-click="clearInput()">
                  <option value="">-- Selecione --</option>                                                    
                </select>
              </div>
              <div class="form-group col-sm-12 col-md-3">
                <label >Data Início *</label>                    
                <input type="text" class="form-control data-locacao" name="dataInicio" id="dataInicio" ng-model="proposta.dataInicio" ng-change="periodoLocacao(proposta.dataInicio)" autocomplete="off" ng-click="clearInput()"/>
              </div>
              <div class="form-group col-sm-12 col-md-3">
                <label >Data Final *</label>
                <input type="text" class="form-control data-locacao" name="dataFim" id="dataFim" autocomplete="off" ng-model="proposta.dataFim" disabled >
              </div>
              <div class="form-group col-sm-12 col-md-6">
                <label >Local Retirada *</label>
                <select class="form-control" name="localRetirada" id="localRetirada" ng-model="proposta.localRetirada" ng-click="clearInput()">
                  <option value="" selected>-- Selecione --</option>
                  <option value="1">Campinas</option>
                  <!-- <option value="2">São Paulo</option>  -->                                                
                </select>                        
              </div>
              <div class="col-md-12" style="text-align: center; margin-top: 5vh">
                <button  id="btn-cadastro-proposta"  ng-click="cadastroUsuarioLocacao();" style="width: 180px;">Enviar</button>
                <br>
                <div id="txt-aguarde-login"></div> 
              </div>      
              <div class="row" id="bloco-2" style="display: none">
                <div class="col-sm-12 col-md-4">
                  <p>Veículo </p>
                  <p ng-if="proposta.tipoVeiculo == 2">Proteção </p>
                </div>
                <div class="col-sm-12 col-md-8">
                  <p>{{simulacao.tipoVeiculo}}</p>
                  <p ng-if="proposta.tipoVeiculo == 2">{{simulacao.tipoProtecao}}</p>
                </div>                     
              </div>
            </form>   
          </div>
        </div>
      </div>
    </div>

    <!-- ***********##################### BLOCO SOU CLIENTE ##################****************--> 
    <div class="modal-body" id="bloco2" >
      <div class="container login-container"  style="width: 100%" >
        <div class="row">
          <div class="col-md-12 formulario" >
            <div class="col-md-6 login-form-1">
              <h3>Já sou cliente</h3>
              <form>
                <div class="form-group">
                  <input id="login-username2" type="text" class="form-control" name="username2" placeholder="CPF"  required ng-model="validation.cpf">
                </div>
                <div class="form-group">
                 <input id="login-password3" type="password" class="form-control" name="password3" placeholder="Senha" required ng-model="validation.pass">
               </div>
               <div class="form-group">
                <input type="submit" id="logUser2" class="btnSubmit" value="Entrar" ng-click="logUser()"/>
                <br>
                <div id="txt-aguarde-loguser"></div> 
              </div>
              <div class="form-group">
                <span class="pull-left">
                  <a href="#" data-ng-click="vm.signIn=false;" >Esqueceu sua Senha?</a>
                </span>
              </div>
            </form>
          </div>
          <div class="col-md-6 login-form-2">
            <h3>Quero criar uma conta</h3>
            <form>
              <div class="form-group">
                <input type="hidden"  ng-model="login.codUsuario" name="login-codUsuario" id="login-codUsuario"> 
                <input type="text"  ng-model="login.cpf" name="cpf" id="cpf"  placeholder="CPF" class="form-control" ui-br-cpf-mask required>                                     
              </div>
              <div class="form-group">
               <input id="login-password4" type="password" class="form-control" name="password4" placeholder="Senha" ng-model="login.pass1" required>
             </div>
             <div class="form-group">
               <input id="login-password5" type="password" class="form-control" name="password5" placeholder="Confirmar Senha" ng-model="login.pass2" required>
