                              ng-class="{'is-invalid': form.cpf.$invalid && form.cpf.$touched,'is-valid': form.cpf.$valid}" required>
                              <p ng-show="form.cpf.$invalid && !form.cpf.$pristine" class="help-block">Digite um cpf válido.</p>
