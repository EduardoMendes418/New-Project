                              ng-class="{'is-invalid': form.email.$invalid && form.email.$touched,'is-valid': form.email.$valid}" required>
                              <p ng-show="form.email.$invalid && !form.email.$pristine" class="help-block">Digite um e-mail válido.</p>
                          </div>
                      </div>
