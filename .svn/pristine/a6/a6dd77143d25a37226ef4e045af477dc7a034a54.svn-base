    <div class="modal fade" id="contato" tabindex="-1" role="dialog" aria-labelledby="contato" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <a class="py-2" href="https://www.goeyja.com.br/#home">               
                    <img src="images/home/logo-goey.png">
                </a>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <!--FORMULARIO DE CONTATO-->
                <form name="form" class="form" novalidate>
                <div class="form-group">
                    <input class='form-control ' placeholder="Nome:" required ng-model="demail.nome" type='text' name="nome" onkeypress="return SomenteLetra(event);" ng-class="{'is-invalid': form.nome.$invalid && form.nome.$touched,'is-valid': form.nome.$valid}" >
                    <p ng-show="form.nome.$invalid && form.nome.$touched" style="color:red;font-size:10px !important;">Nome obrigatório</p>
                </div>
                
                <div class="form-group">
                        <input class='form-control ' placeholder="Email:" required ng-model="demail.email" type='email' name="email" ui-br-email-mask 
                        onkeypress="return SomenteLetra(event);" ng-class="{'is-invalid': form.email.$invalid && form.email.$touched,'is-valid': form.email.$valid}">
                        <p ng-show="form.email.$invalid && form.email.$touched" style="color:red;font-size:10px !important;">E-mail obrigatório</p>
                    </div>

                <div class="form-group">
                    <input class='form-control number' placeholder="Telefone:" required ng-model="demail.telefone" type='text' name="telefone" ui-br-phone-number
                    ng-class="{'is-invalid': form.telefone.$invalid && form.telefone.$touched,'is-valid': form.telefone.$valid}">
                    <p ng-show="form.telefone.$invalid && form.telefone.$touched" style="color:red;font-size:10px !important;"> Telefone obrigatório</p>
                </div>

                <div class="form-group">
                    <textarea class="form-control" placeholder="Mensagem:" ng-model="demail.desc" rows="8"
                    ng-class="{'is-invalid': form.desc.$invalid && form.desc.$touched,'is-valid': form.desc.$valid}">
                    <p ng-show="form.desc.$invalid && form.desc.$touched" style="color:red;font-size:10px !important;">Mensagem obrigatório</p>
                    </textarea>
                </div>
                
                <!--Telefone-->
                <div class="telefone">
                        <div class="row">
                            <div class="col-12 col-sm-6 col-lg-5">
                                    <span href="#">Telefone: (19) 98187-0077</span>
                            </div>
                            <div class="col-12 col-sm-6 col-lg-3"></div>
                            <div class="col-12 col-sm-6 col-lg-4"></div>    
                        </div>
                        <div class="row">
                            <div class="col-12 col-sm-6 col-lg-5">
                                    <span href="#">Contato: 0800 941 3950</span>
                            </div>
                            <div class="col-12 col-sm-6 col-lg-3"></div>
                            <div class="col-12 col-sm-6 col-lg-4"></div>    
                        </div>
                    </div> 
                </div>

                <div class="modal-footer" style="background:#f48022 ">
                    <button type="submit" class="btn btn-primary" ng-click="enviarEmail();" ng-disabled="form.$invalid" ng-show="!cadTrue">Enviar Mensagem</button>
